<html>
    <head>
        <title>Testimonial</title>
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.2/css/all.min.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    </head>
    <body>
        <nav class="navbar navbar-expand-lg bg-body-tertiary">
            <div class="container-fluid">
                <a class="navbar-brand" href="#">
                    <img src="/images/logo.png" alt="cikhidayat" width="50">
                </a>
              <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                  <li class="nav-item">
                    <a class="text-center nav-link link-opacity-10-hover" style="" aria-current="page" href="/">Home</a>
                  </li>
                  {{#if isLogin}}
                  <li class="nav-item">
                    <a class="text-center nav-link link-opacity-10-hover" href="/add-blog">Add Blog</a>
                  </li>
                  {{/if}}
                  <li class="nav-item">
                    <a class="text-center nav-link link-opacity-10-hover" href="/contact">Contact Me</a>
                  </li>
                  <li class="nav-item">
                    <a class="text-center nav-link link-opacity-10-hover fw-medium active" href="/testimonial">Testimonial</a>
                  </li>
                </ul>
                <div class="d-flex justify-content-center m-0">
                  {{#if isLogin}}
                  <a class="btn btn-dark m-2" href="#">Hello, {{user}}</a>
                  <a class="btn btn-primary m-2" href="/logout">Logout</a>
                  {{else}}
                  <a class="btn btn-dark m-2" href="/register">Register</a>
                  <a class="btn btn-primary m-2" href="/login">Login</a>
                  {{/if}}
                </div>
              </div>
            </div>
          </nav>
        <div class="container">
            <h3 class="mt-4 text-center">TESTIMONIAL</h3>
            <h5 class="text-center mt-4">Rating Your Worker</h5>
            <div class="d-flex flex-sm-row justify-content-around my-4">
                <button class="btn btn-primary" onclick="showTest()">All</button>
                <button class="btn btn-primary" onclick="filterData(1)">1 <i class="fa-solid fa-star"></i></button>
                <button class="btn btn-primary" onclick="filterData(2)">2 <i class="fa-solid fa-star"></i></button>
                <button class="btn btn-primary" onclick="filterData(3)">3 <i class="fa-solid fa-star"></i></button>
                <button class="btn btn-primary" onclick="filterData(4)">4 <i class="fa-solid fa-star"></i></button>
                <button class="btn btn-primary" onclick="filterData(5)">5 <i class="fa-solid fa-star"></i></button>
            </div>
            <div id="test-content" class="d-sm-flex container justify-content-around py-3 flex-wrap">
                <!-- test content -->
            </div>
        </div>
        <div>
            <footer class="border bg-dark m-0 p-3">
              <p class="text-light m-0 text-center">Copyright 2023</p>
              <p class="text-light m-0 text-center">Created by Rahmat Hidayat | <a class="text-light mx-1" href="https://instagram.com/rhmthidayat07"><i class="fa-brands fa-instagram"></i></a>
                    <a class="text-light mx-1" href="https://www.facebook.com/rahmathidayat674"><i class="fa-brands fa-facebook"></i></a>
                    <a class="text-light mx-1" href="https://www.linkedin.com/in/rhidayat1407/"><i class="fa-brands fa-linkedin-in"></i></i></a>
                    <a class="text-light mx-1" href="https://twitter.com/rhmthidayat07"><i class="fa-brands fa-twitter"></i></a></p>
            </footer>
          </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
        <script src="/js/testimonial-ajax.js"></script>
        <script src="/js/button.js"></script>
    </body>
</html>